    /* Login / Register Page Styles
    **************************************************************/

    #page-header + .account-login {
        margin-top: 30px;
    }

    .account-login #account-box {
        margin-top: 20px;
        margin-bottom: 20px;
    }

    .account-login #account-box .account-box-inner {
        padding-top: 20px;
        padding-bottom: 20px;
    }

    .account-login #account-box .account-box-inner > p {
        text-align: justify;
    }

    #login-heading {
        margin: 0px;
        color: #895c9a;
        font-family: 'Tahoma', Helvetica, "Helvetica Neue", Arial, sans-serif;
        /*font-variant: small-caps;*/
        font-size: 32px;
        line-height: 32px;
    }

    .account-create label.validation-failed,
    .account-create .validation-failed ~ label.required {
        color: #F84A33;
    }

    .account-create input.validation-failed {
        background-color: rgb(255, 224, 145) !important;
    }

    /* Account Dashboard Styles
    *****************************************************************/

    .box-head {
        position: relative;
        border-bottom: 1px solid #e6e6e6;
        margin: 10px;
        padding-left: 20px;
    }

    .box-head > h1,
    .box-head > h2,
    .box-head > h3 {
        margin-bottom: 10px;
    }

    .box-head > * {
        color: #444;
        font-family: 'Tahoma', Helvetica, "Helvetica Neue", Arial, sans-serif;
        font-size: 22px;
        line-height: 26px;
    }

    .box-head > * > a {
        text-decoration: none;
    }

    .box-head > * > a:hover {
        text-decoration: underline;
    }

    .box-head > .box-head-timestamp,
    .box-head > * + .required,
    .box-title > * + .required {
        position: relative;
        top: 3px;
        right: 20px;
        font: normal 14px/22px "Tahoma";
