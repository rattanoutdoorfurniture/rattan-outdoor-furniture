        height: 74px;
        background: #FFF url('../images/checkout-steps-progress.png') 0px 0px no-repeat;
    }

    #checkout-box.step1 #checkout-box-progress-inner { background-position: 0px 0px; }
    #checkout-box.step2 #checkout-box-progress-inner { background-position: 0px -75px; }
    #checkout-box.step3 #checkout-box-progress-inner { background-position: 0px -150px; }

    .checkout-box-progress-step {
        display: inline-block;
        width: 177px;
        height: 100%;
    }

    #login-form,
    #checkout-box-form {
        margin: 0;
        padding: 10px 0 20px;
    }

    .checkout-box-step {
        padding: 0 20px;
    }

    .checkout-box-step:after { content: "\00D\00A"; display: block; height: 0; clear: both; visibility: hidden; }


    #checkout-box.step1 .checkout-box-step,
    #checkout-box.step2 .checkout-box-step,
    #checkout-box.step3 .checkout-box-step,
    #checkout-box.step4 .checkout-box-step {
        display: none;
    }

    #checkout-box.step1 #checkout-box-step-1,
    #checkout-box.step2 #checkout-box-step-2,
    #checkout-box.step3 #checkout-box-step-3,
