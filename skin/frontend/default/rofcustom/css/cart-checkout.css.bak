/*********************************\
|* Cart and Checkout Page Styles *|
\*********************************/

/* Page Heading
*************************************************************************/

.page-title {
    position: relative;
}

.page-title h1,
#page-heading,
#search-heading,
#account-heading,
#cart-heading {
    border-bottom: 1px solid #e6e6e6;
    color: #895c9a;
    font-family: 'Tahoma', Helvetica, "Helvetica Neue", Arial, sans-serif;
    text-transform: uppercase;
    margin: 10px 0px 20px;
    font-size: 22px;
    line-height: 22px;
    padding-bottom: 10px;
}

.page-title aside {
    position: absolute;
    top: 10px;
    right: 15px;
}

.page-title aside a:hover {text-decoration: underline;}
.page-title aside a {
    text-decoration: none;
    color: #888;
    font-variant: small-caps;
    text-transform: lowercase;
    font-size: 1.1em;
    display: inline-block;
}
.page-title aside.back-aside:hover {border-bottom: 1px solid #888;}
.page-title aside.back-aside a:hover {text-decoration:none;}

.page-title aside.back-aside.normal-hover:hover {border-bottom: none}
.page-title aside.back-aside.normal-hover a:hover {text-decoration: underline;}

img, button, input {
    outline: 0 none;
}

/* Cart Box class styles (boxes for cart content are styled similarly)
*************************************************************************/
.account-box,
.cart-box {
    min-height: 40px;
    position: relative;
    background: transparent url('../images/hash-border-darker.png') 0 0 repeat;
    height: auto;
    page-break-after: always;
    page-break-inside: avoid;
}

.account-box-border,
.cart-box-border {
    width: 100%;
    height: 10px !important;
    display: block;
}

.account-box .account-box-inner,
.cart-box  .cart-box-inner {
    background:#FFF;
    display: block;
    margin: auto 10px;
    overflow-x: hidden;
    overflow-y: auto;
}

.account-box .account-box-inner:before,
.account-box .account-box-inner:after,
.cart-box .cart-box-inner:before,
.cart-box .cart-box-inner:after { content: "\00D\00A"; display: block; height: 0; clear: both; visibility: hidden; width: 0; overflow: hidden; }

.cart-btn-wrap {
    text-align: center;
    float: left;
    margin-top: 10px;
    margin-bottom: 10px;
}

