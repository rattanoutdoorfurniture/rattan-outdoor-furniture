    function cleanCsvValue(&$lval,$lkey, $unaccent) {
        $lval = str_replace(array_keys($unaccent), array_values($unaccent), $lval);
        $lval = preg_replace('/^([^\d]*)(\d)\-/',"$1$2 ",$lval);
        return filter_var($lval, FILTER_SANITIZE_SPECIAL_CHARS, array(FILTER_FLAG_ENCODE_LOW,FILTER_FLAG_ENCODE_HIGH));
    }
    function multi_unique($array) {
        $new = array();
        foreach ($array as $k=>$na)
            $new[$k] = serialize($na);
        $uniq = array_unique($new);
        $new1 = array();
        foreach($uniq as $k=>$ser)
            $new1[$k] = unserialize($ser);
        return ($new1);
    }
    function truncate($string,$length=100,$append="&hellip;") {
        $string = trim($string);

        if(strlen($string) > $length) {
            $string = wordwrap($string, $length);
            $string = explode("\n",$string);
            $string = array_shift($string) . $append;
        }

        return $string;
    }

    function phoneLink($phone) {
        $phoneStrip = preg_filter('/[^0-9A-Za-z]+/','',$phone);
        global $io;
        $phoneTable = array(
            2   => array("a","b","c"),
            3   => array("d","e","f"),
            4   => array("g","h","i"),
            5   => array("j","k","l"),
            6   => array("m","n","o"),
            7   => array("p","q","r","s"),
            8   => array("t","u","v"),
            9   => array("w","x","y","z")
        );
        $phoneDigits = str_split($phoneStrip);
        $phoneNumber = '';
        foreach($phoneDigits as $phoneDigit) {
            if(is_numeric($phoneDigit)) {
                $phoneNumber.= $phoneDigit;
                continue;
            }
            foreach($phoneTable as $phoneBtn=>$btnValues) {
                if(in_array(strtolower($phoneDigit),$btnValues)) {
                    $phoneNumber .= $phoneBtn;
                    break;
                }
            }
        }
        if($phoneNumber[0]!=="1") {
            $phoneNumber = "1" . $phoneNumber;
        }

        return '<a href='.$io['q'].'tel:'.$phoneNumber.$io['q'].'>'.$phone.'</a>';
    }

    function getPhoneLinkNumber($phone = NULL) {
        if(is_null($phone)) $phone = Mage::getStoreConfig('general/store_information/phone');
